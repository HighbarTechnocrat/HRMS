        <script type="text/javascript">

            $(document).ready(function () {
                // Create jqxRating.
                $("#jqxRating").jqxRating({ height: 35, value: document.getElementById("MainContent_pdnewglobal_m_uxPdLayout_ctl04_hdvalue2").value });
                // bind to jqxRating 'change' event.
                $("#jqxRating").bind('change', function (event) {
                    document.getElementById("MainContent_pdnewglobal_m_uxPdLayout_ctl04_hdvalue2").value = event.value;
                });
            });
        </script>

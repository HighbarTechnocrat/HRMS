        <script>
            $(window).scroll(function () {
                if ($(window).scrollTop() > 100) {
                    $('.header').addClass('sticky-show-navigation fixed-navigation');
                } else {
                    $('.header').removeClass('sticky-show-navigation fixed-navigation');
                    $('.header').addClass('fixed-navigation-hidden sticky-show-navigation');
                }
            });
        </script>

